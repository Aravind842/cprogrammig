#include <stdio.h>
#include <conio.h>
void main()
{
	int num,res=0;
	clrscr();
	printf("\enter the number;");
	scanf("%d",&num);
	res=prime(num);
	if(res==0)
	printf("\n%dis theprime number");
	else
	printf("\n%d is not a prime number");
	getch();
}
int prime(int n)
{
	int i;
	for(i=2;i<=n/2;i++)
	{
	if(n%i!=0)
	continue;
	else
	return 1;
	}
	return 0;
}
